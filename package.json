{
  "name": "mcp-docker-exec",
  "version": "1.0.0",
  "description": "MCP server for Docker container execution with streaming support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "dockerode": "^4.0.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "nanoid": "^5.0.4",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}